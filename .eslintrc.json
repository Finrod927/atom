{
	"extends": "@alhadis/eslint-config/atom",
	"rules": {
		"prefer-template": 0,
		"operator-linebreak": 0,
		"no-underscore-dangle": [
			"error",
			{
				"allowAfterThis": true,
				"allow": ["_FileIcons"]
			}
		]
	},
	"overrides": [{
		"files": ["lib/ui.js"],
		"rules": {"space-before-function-paren": 0}
	},{
		"files": ["test/*.js"],
		"rules": {
			"space-in-parens": 0
		}
	}]
}
